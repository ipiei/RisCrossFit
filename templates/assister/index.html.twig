{% extends 'base.html.twig' %}

{% block title %}Assister index{% endblock %}

{% block body %}
    <h1>Assister index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for assister in assisters %}
            <tr>
                <td>{{ assister.id }}</td>
                <td>
                    <a href="{{ path('assister_show', {'id': assister.id}) }}">show</a>
                    <a href="{{ path('assister_edit', {'id': assister.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('assister_new') }}">Create new</a>
{% endblock %}
